<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.Controller">
   <children>
      <AnchorPane fx:id="background" layoutX="14.0" layoutY="-7.0" prefHeight="413.0" prefWidth="608.0">
         <children>
            <Button fx:id="playbutton" layoutX="304.0" layoutY="251.0" mnemonicParsing="false" onAction="#play" text="Play">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></Button>
            <Button fx:id="loadbutton" layoutX="368.0" layoutY="251.0" mnemonicParsing="false" onAction="#loadRom" text="Load ROM">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></Button>
            <Label fx:id="choose" blendMode="SRC_ATOP" layoutX="32.0" layoutY="64.0" prefHeight="17.0" prefWidth="81.0" />
            <ComboBox fx:id="comboBox" layoutX="32.0" layoutY="51.0" onAction="#changeTheme" prefWidth="150.0" promptText="Choose Theme" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
